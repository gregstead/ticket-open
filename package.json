{
  "name": "ticket-open",
  "version": "1.0.0",
  "description": "An open source nonprofit management system",
  "engines": {
    "node": "12.18.3"
  },
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production  &&npm run start:prod || npm run start:dev",
    "start:prod": "node ./server/server.js",
    "start:dev": "concurrently \"nodemon ./server/server.js\" \"npm run client\"",
    "client": "cd client && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gregstead/ticket-open.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gregstead/ticket-open/issues"
  },
  "homepage": "https://ticketopen.herokuapp.com/",
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
    "concurrently": "^6.1.0",
    "if-env": "^1.0.4",
    "kill-port": "^1.6.1",
    "react-feather": "^2.0.9",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "bcryptjs": "2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "passport-session": "^1.0.2",
    "sequelize": "^6.6.2",
    "validator": "^13.6.0"
  }
}
